{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preversion": "echo 'Cannot add tag in /back-end subfolder' && exit 1",
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier src/**/*.ts --write"
  },
  "dependencies": {
    "@tsed/ajv": "6.114.14",
    "@tsed/common": "6.114.14",
    "@tsed/core": "6.114.14",
    "@tsed/di": "6.114.14",
    "@tsed/exceptions": "6.114.14",
    "@tsed/json-mapper": "6.114.14",
    "@tsed/mongoose": "6.114.14",
    "@tsed/platform-express": "6.114.14",
    "@tsed/schema": "6.114.14",
    "ajv": "^8.9.0",
    "bcrypt": "^5.0.1",
    "compression": "1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "1.4.5",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.4.0",
    "method-override": "3.0.0",
